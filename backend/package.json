{
  "name": "youtube-transcript-backend",
  "version": "1.0.0",
  "description": "Backend API for YouTube transcript and description extraction",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "INTEGRATION_TESTS=true jest --testPathPattern=integration",
    "test:endpoints": "jest --testPathPattern=endpoints",
    "test:middleware": "jest --testPathPattern=middleware",
    "test": "jest --testPathPattern=endpoints --testPathPattern=middleware",
    "test:verbose": "VERBOSE_TESTS=true npm run test --verbose",
    "test:ai": "jest --testPathPattern=\"ai\"",
    "test:ai:coverage": "jest --testPathPattern=\"ai\" --coverage",
    "test:ai:watch": "jest --testPathPattern=\"ai\" --watch",
    "test:load": "node scripts/transcript-tester.js",
    "test:load:heavy": "node scripts/transcript-tester.js --runs 10"
  },
  "keywords": [
    "youtube",
    "transcript",
    "api",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "request-ip": "^3.3.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
